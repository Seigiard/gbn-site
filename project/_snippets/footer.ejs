<%
  const hash = new Date().getTime();
%>

<script src="/js/vendor/jquery-3.2.1.min.js"></script>
<script src="/js/vendor/inputmask/inputmask.min.js"></script>
<script src="/js/vendor/inputmask/jquery.inputmask.min.js"></script>
<script src="/js/vendor/savy.min.js?<%- hash %>"></script>
<script src="/js/vendor/stickyfill.js?<%- hash %>"></script>
<script src="/js/vendor/rangeslider.js-2.3.0/rangeslider.min.js"></script>
<script src="/js/PriceEditor.js?<%- hash %>"></script>
<script src="/js/plugins.js?<%- hash %>"></script>
<script src="/js/main.js?<%- hash %>"></script>

<% if(!!locals.priceEditor) { %>
<script>
  var priceEditor = new PriceEditor({
    <% if(priceEditor.maxInputValue) { %>maxInputValue: <%- priceEditor.maxInputValue %>, <% } %>
    <% if(priceEditor.minCommissionValue) { %>minCommissionValue: <%- priceEditor.minCommissionValue %>, <% } %>
    commissionRange: <%- JSON.stringify(priceEditor.commissionRange) %>
  });
</script>
<% } %>